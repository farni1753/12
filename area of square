import turtle as t

class moraba():
    def __init__(self,l):
        self.lenght=l

    def moraba_area(self):
        return self.lenght*self.lenght
    def draw(self):
        for i in range(1,5):
            t.fd(100)
            t.lt(90)        

newmoraba=moraba(100)                            
print(newmoraba.moraba_area())
newmoraba.draw()
